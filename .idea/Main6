import java.util.Arrays;
        import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        while (sc.hasNext()) {
        int k = sc.nextInt();
        int n = 1;
        double sum=0;
        while(sum<=k){
            sum+=1.0/n;
            n++;
        }
            System.out.println(n-1);

        }
    }
}
